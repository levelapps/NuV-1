{
  "expo": {
    "name": "NüV",
    "description": "A 'life(style) support system' for vegans and vegetarians. A communal hub for all things that concern the ethical and healthy eaters among us",
    "slug": "nuv",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/AppIcons/letter-n.png",
    "splash": {
      "image": "./assets/AppIcons/cleansplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.levelapps.nuv",
      "infoPlist": {
      "NSPhotoLibraryUsageDescription": "NüV needs to access your photo library to save a photo which shows you at your best."
    },
    },
    "android": {
     "package": "com.levelapps.nuv",
     "versionCode": 3.0,
     "permissions": ["ACCESS_COARSE_LOCATION",
      "ACCESS_FINE_LOCATION",
      "CAMERA"]

   }
  }
}
